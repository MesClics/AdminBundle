{
    "name": "mesclics/admin-bundle",
    "description": "Bundle pour l'ajout d'une console d'administration",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {},
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": ">=5.5.9",
        "mesclics/message-bundle": "*",
        "mesclics/post-bundle": "*",
        "mesclics/user-bundle": "*",
        "mesclics/utils-bundle": "*"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "mesclics/admin-bundle",
                "version": "master",
                "source": {
                    "url": "https://github.com/MesClics/AdminBundle.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "mesclics/message-bundle",
                "version": "master",
                "source": {
                    "url": "https://github.com/MesClics/MessagesBundle.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "mesclics/post-bundle",
                "version": "master",
                "source": {
                    "url": "https://github.com/MesClics/PostBundle.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "mesclics/user-bundle",
                "version": "master",
                "source": {
                    "url": "https://github.com/MesClics/UserBundle.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "mesclics/utils-bundle",
                "version": "master",
                "source": {
                    "url": "https://github.com/MesClics/UtilsBundle.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "FOS\\CKEditorBundle\\Composer\\CKEditorScriptHandler::install",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": null
    }
}